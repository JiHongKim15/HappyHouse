<style>
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.pinktable {
  text-align: center;
  line-height: 40px;
  border-collapse: separate;
  border-spacing: 0;
  border: 2px solid #f82a6c;
  width: 100%;
  border-radius: .25rem;
  margin: 10px;
}


.pinktable td{
	 width:1%;
       white-space:nowrap;
}

.pinktable thead tr:first-child {
  background: #f82a6c;
  color: #fff;
  border: none;
  height: 30px;
  text-align: center;
}

.pinktable th:first-child,
.pinktable td:first-child {
  padding: 0 15px 0 20px;
  height: 55px;
}

.pinktable th {
  font-weight: 500;
  padding: 0 0 0 10px;
}


.pinktable tbody tr:hover {
  background-color: #f2f2f2;
  cursor: default;
}


.pinktable tbody td {
  border-bottom: 1px solid #ddd;
}

.pinktable td:last-child {
  padding-right: 10px;
}

.button {
  color: #aaa;
  cursor: pointer;
  vertical-align: middle;
  margin-top: -4px;
}

.edit:hover {
  color: #0a79df;
}

.delete:hover {
  color: #dc2a2a;
}

/*세로형 테이블*/

.pinktable2 {
  text-align: left;
  line-height: 40px;
  border-collapse: separate;
  border-spacing: 0;
  border: 2px solid #f82a6c;
  margin: 10px;
  border-radius: .25rem;
  padding: 0;
}


.pinktable2 tr th:first-child {
  background:#f82a6c;
  color: #fff;
  border: none;
}

.pinktable2 th:first-child,
.pinktable2 td:first-child {
  padding: 0 15px 0 20px;
}

.pinktable2 td {
  font-weight: 500;
  padding: 0 0 0 10px;
}

.pinktable2 th {
  font-weight: 500;
  padding: 0 15px 0 20px;
}



.pinktable2 thead td:hover {
  background-color: #f2f2f2;
  cursor: default;
}


/*input style!!*/

.myinput {
  border-top : 1px solid #fff;
  border-left : 1px solid #fff;
  border-right : 1px solid #fff;
  border-bottom: 1px solid #f82a6c;
  margin: 1px;
  padding: 15px;
}

.myinput input.form-control[type=text]:focus:not([readonly]) {
  border-bottom: 1px solid #f48fb1;
  box-shadow: 0 1px 0 0 #f48fb1;
}
.myinput input.form-control[type=text] {
  border-bottom: 1px solid #f48fb1;
  box-shadow: 0 1px 0 0 #f48fb1;
}
.myinput input.form-control[type=text]:focus:not([readonly]) {
  border-bottom: 1px solid #ce93d8;
  box-shadow: 0 1px 0 0 #ce93d8;
}
.myinput input.form-control[type=text] {
  border-bottom: 1px solid #ce93d8;
  box-shadow: 0 1px 0 0 #ce93d8;
}
.myinput input.form-control[type=text]:focus:not([readonly]) {
  border-bottom: 1px solid #4dd0e1;
  box-shadow: 0 1px 0 0 #4dd0e1;
}
.myinput input.form-control[type=text] {
  border-bottom: 1px solid #4dd0e1;
  box-shadow: 0 1px 0 0 #4dd0e1;
}
.myinput .fa, .active-cyan-2 .fa {
  color: #4dd0e1;
}
.myinput .fa, .active-purple-2 .fa {
  color: #ce93d8;
}
.myinput .fa, .active-pink-2 .fa {
  color: #f48fb1;
}

